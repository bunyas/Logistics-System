
@{
    ViewBag.Title = "ContactPerson";
    Layout = "~/Views/Shared/_LayoutHSIP.cshtml";
}

<h2>Contact Persons</h2>
@(Html.EJ().Dialog("basicDialog").Title("Warning")@*.ShowFooter(true).FooterTemplateId("sample")*@.ShowOnInit(false))

<div id="ControlRegion">
    @(Html.EJ().Grid<object>("Grid")
         .Datasource(ds => ds.URL("DataSourceContacts").UpdateURL("DialogInsert").InsertURL("DialogInsert").RemoveURL("DialogDelete").Adaptor(AdaptorType.UrlAdaptor))

       // .Datasource(datasource => datasource.URL("GetComplaints")/*.UpdateURL("DialogUpdate").InsertURL("DialogInsert")*/.RemoveURL("DialogDelete").Adaptor(AdaptorType.UrlAdaptor))
       .AllowSearching(true)
    .EditSettings(edit => { edit.AllowAdding().AllowDeleting().AllowEditing().EditMode(EditMode.DialogTemplate).DialogEditorTemplateID("#template"); })

    .ContextMenuSettings(contextMenu =>
    {
        contextMenu.EnableContextMenu();
    }).AllowScrolling()
    .ScrollSettings(col => { col.Width(1300)/*.Height(700)*/; })
    .AllowPaging()
     .AllowFiltering().ShowColumnChooser().AllowResizing()
    .FilterSettings(filter => { filter.FilterType(FilterType.Excel); })
    .ToolbarSettings(toolbar =>
    {
        toolbar.ShowToolbar().ToolbarItems(items =>
        {
            items.AddTool(ToolBarItems.Add);
            items.AddTool(ToolBarItems.Edit);
            items.AddTool(ToolBarItems.Update);
            items.AddTool(ToolBarItems.Cancel);
            items.AddTool(ToolBarItems.Search);
        });
    })
    .Columns(col =>
    {
        col.Field("cp_code").HeaderText("cp_code").IsPrimaryKey(true).Visible(false).TextAlign(TextAlign.Justify).Width(100).Add();
        col.Field("cp_name").HeaderText("Names").Visible(true).TextAlign(TextAlign.Justify).Width(200).Add();
        col.Field("AddedBy").HeaderText("AddedBy ").Visible(false).TextAlign(TextAlign.Justify).Width(100).Add();
        col.Field("AddedDate").HeaderText("AddedDate").Visible(false).TextAlign(TextAlign.Justify).Width(100).Add();

        col.Field("User_ID").HeaderText("User Name").ForeignKeyField("Id").ForeignKeyValue("UserName").DataSource((IEnumerable<object>)ViewBag.Users).Visible(false).TextAlign(TextAlign.Justify).Width(200).Add();
        //col.Field("UserId").HeaderText("UserId").ForeignKeyField("Id").ForeignKeyValue("UserName").DataSource((IEnumerable<object>)ViewBag.Users).Visible(false).TextAlign(TextAlign.Justify).Width(100).Add();
        col.Field("cp_title").HeaderText("Title").ForeignKeyField("title_code").ForeignKeyValue("title_desc").DataSource((IEnumerable<object>)ViewBag.Titles).Visible(true).TextAlign(TextAlign.Justify).Width(200).Add();
        col.Field("cp_facility").HeaderText("Health Facility").ForeignKeyField("FacilityCode").ForeignKeyValue("Facility").DataSource((IEnumerable<object>)ViewBag.Facilities) .Visible(true).TextAlign(TextAlign.Justify).Width(200).Add();
        col.Field("cp_district").HeaderText("District").ForeignKeyField("District_Code").ForeignKeyValue("District_Name").DataSource((IEnumerable<object>)ViewBag.Districts).Visible(true).TextAlign(TextAlign.Justify).Width(150).Add();
        col.Field("cp_category").HeaderText("Category") .ForeignKeyField("category_id").ForeignKeyValue("category_desc").DataSource((IEnumerable<object>)ViewBag.ContactCategories).Visible(true).TextAlign(TextAlign.Justify).Width(150).Add();
        col.Field("cp_ip").HeaderText("Implementing Partner").ForeignKeyField("ImplimentingPartnerCode").ForeignKeyValue("ImplementingPartnerDescription").DataSource((IEnumerable<object>)ViewBag.Partners).Visible(true).TextAlign(TextAlign.Justify).Width(150).Add();
        col.Field("cp_supplier_code").HeaderText("Supplier").ForeignKeyField("supplier_code").ForeignKeyValue("supplier_desc").DataSource((IEnumerable<object>)ViewBag.Suppliers).Visible(true).TextAlign(TextAlign.Justify).Width(150).Add();
        col.Field("ct_telephon").HeaderText("Tel. No 1") .Visible(true).TextAlign(TextAlign.Justify).Width(150).Add();
        col.Field("ct_telephone_2").HeaderText("Tel. No 2") .Visible(true).TextAlign(TextAlign.Justify).Width(150).Add();
        col.Field("ce_email").HeaderText("Email") .Visible(true).TextAlign(TextAlign.Justify).Width(250).Add();
        col.Field("Personal_email").HeaderText("Personal email").Visible(true).TextAlign(TextAlign.Justify).Width(250).Add();
        col.Field("IsActive").HeaderText("Is Active").Visible(true).TextAlign(TextAlign.Justify).Width(250).Add();

    })
    .ClientSideEvents(eve => { /*eve.Load("onLoad");*/ eve.ActionComplete("edit"); }))

</div>


