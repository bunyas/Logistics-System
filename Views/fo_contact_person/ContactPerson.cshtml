<script>
    function edit(args) {

        if (args.requestType == "beginedit" || args.requestType == "add") {
            $($("#EditDialog_Grid_Save").hide()).after('<button id="save" type="button" onclick="savess()" class="btn btn-success btn-sm" > <span class="glyphicon glyphicon-save"></span> Save</button>');
            $("#save").css({ "width": "100px", "height": "50px", "font-size": "17px", "float": "right", "margin-left": "10px", "margin-top": "20px", "margin-bottom": "10px" });
            $("#EditDialog_Grid_Cancel").ejButton("destroy");
            $($("#EditDialog_Grid_Cancel").hide()).after(' <button type="button" id="cancel" onclick="cancelss()" class="btn btn-danger btn-sm" ><span class="glyphicon glyphicon-remove-circle"></span> Cancel</button>');
            $("#cancel").css({ "width": "100px", "height": "50px", "float": "right", "margin-left": "10px", "margin-top": "20px", "margin-bottom": "10px" });

            $("#AddedDate").ejDatePicker(
                {
                    /*maxDate: new Date(),*/
                    value: new Date(new Date($("#AddedDate").attr("value"))),
                    //validationRules: {
                    //    required: true
                    //},
                    //validationMessage: {
                    //    required: "Date recieved is Required"
                    //},
                }
            );


            var users=@Html.Raw(Json.Encode(ViewBag.Users));
            var YesNo =@Html.Raw(Json.Encode(ViewBag.YesNo));
            var Facilities =@Html.Raw(Json.Encode(ViewBag.Facilities ));
            var contacts =@Html.Raw(Json.Encode(ViewBag.Contacts));
            var maul_Services =@Html.Raw(Json.Encode(ViewBag.MAUL_Service));

            var ASDropDownList = $('#cp_facility').ejDropDownList({
                dataSource: Facilities,
                width: "240px",
                fields: { value: "FacilityCode", text: "Facility" }, readOnly: false

            }).data("ejDropDownList");

            var usersList = $('#User_ID').ejDropDownList({
                dataSource: users,
                width: "240px",
                fields: { value: "Id", text: "UserName" },

            }).data("ejDropDownList");

            var titlesList = $('#cp_title').ejDropDownList({
                dataSource: @Html.Raw(Json.Encode(ViewBag.Titles)),
                width: "240px",
                fields: { value: "title_code", text: "title_desc" },
            }).data("ejDropDownList");
            var districtsList = $('#cp_district').ejDropDownList({
                dataSource: @Html.Raw(Json.Encode(ViewBag.Districts)),
                width: "240px",
                fields: { value: "District_Code", text: "District_Name" },
            }).data("ejDropDownList");

            var categoriesList = $('#cp_category').ejDropDownList({
                dataSource: @Html.Raw(Json.Encode(ViewBag.ContactCategories)),
                width: "240px",
                fields: { value: "category_id", text: "category_desc" },
            }).data("ejDropDownList");

            var IPsList = $('#cp_ip').ejDropDownList({
                dataSource: @Html.Raw(Json.Encode(ViewBag.Partners)),
                width: "240px",
                fields: { value: "ImplimentingPartnerCode", text: "ImplementingPartnerDescription" },
            }).data("ejDropDownList");
            var IPsList = $('#cp_supplier_code').ejDropDownList({
                dataSource: @Html.Raw(Json.Encode(ViewBag.Suppliers)),
                width: "240px",
                fields: { value: "supplier_code", text: "supplier_desc" },
            }).data("ejDropDownList");  

            if (args.requestType == "beginedit") {
                var cpname = $('#cp_name').val();
                $('#' + this._id + '_dialogEdit').ejDialog("option", "title", "Edit " + cpname + "'s details"); //change dialog title
                document.getElementById("IsActive").checked = false;
                var gridObj = $("#Grid").data("ejGrid");
                var is_Active = gridObj.getSelectedRecords()[0].IsActive;
                if (is_Active == true) {
                    document.getElementById("IsActive").checked = true;
                }
                //order notifications
                document.getElementById("DisableReceived").checked = false;
                var disableReceived = gridObj.getSelectedRecords()[0].DisableReceived;
                if (disableReceived == true) {
                    document.getElementById("DisableReceived").checked = true;
                }

                document.getElementById("DisableComfirmed").checked = false;
                var disableComfirmed = gridObj.getSelectedRecords()[0].DisableComfirmed;
                if (disableComfirmed == true) {
                    document.getElementById("DisableComfirmed").checked = true;
                }

                document.getElementById("DisableProcessed").checked = false;
                var disableProcessed = gridObj.getSelectedRecords()[0].DisableProcessed;
                if (disableProcessed == true) {
                    document.getElementById("DisableProcessed").checked = true;
                }

                document.getElementById("DisableDispatched").checked = false;
                var disableDispatched = gridObj.getSelectedRecords()[0].DisableDispatched;
                if (disableDispatched == true) {
                    document.getElementById("DisableDispatched").checked = true;
                }
                
                document.getElementById("DisableDelivered").checked = false;
                var disableDelivered = gridObj.getSelectedRecords()[0].DisableDelivered;
                if (disableDelivered == true) {
                    document.getElementById("DisableDelivered").checked = true;
                }
                document.getElementById("DisableAll").checked = false;
                var disableAll = gridObj.getSelectedRecords()[0].DisableAll;
                if (disableAll == true) {
                    document.getElementById("DisableAll").checked = true;
                }
            }
        }

    }

    ///Function to return the dd-mm-YYYY format
      $.validator.setDefaults({
        //to validate all fields(hidden also)
        ignore: [],

        errorClass: 'e-validation-error',
        //it specifies the error message display position
        errorPlacement: function (error, element) {
            $(error).insertAfter(element.closest(".e-widget"));
        },
    });


</script>

