

@{
    ViewBag.Title = "Emergency orders";
    Layout = "~/Views/Shared/_LayoutHSIP.cshtml";
}

<ol class="breadcrumb" style="margin-top:30px;">
    <li class="breadcrumb-item">
        <a href="#">Dashboard</a>
    </li>
    <li class="breadcrumb-item active">Emergency Batch Upload</li>
</ol>
<div class="frame">
    <div class="control">
        <div class="posupload">Select a file to upload</div>
        @Html.EJ().Uploadbox("UploadDefault").SaveUrl(@Url.Action("SaveEmergency")).RemoveUrl(@Url.Action("RemoveEmergency"))
    </div>
</div>
<div id="successmsg">@ViewBag.status</div>
<style class="cssStyles">
    .posupload {
        padding: 10px;
        text-align: center;
    }

    #UploadDefault {
        margin: 0 auto;
    }
</style>



