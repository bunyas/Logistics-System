                "enddate": EndDate,
            },
            //success: function (data) {
            //    var formatData = ej.parseJSON(data);
            //    var grid = $("#Grid").ejGrid("instance");
            //    grid.dataSource(data);
            //    grid.dataSource(formatData);
            //}
            success: function (data) {
                var formatData = ej.parseJSON(data);
                var grid = $("#Grid").ejGrid("instance");
                grid.dataSource(data);
                grid.dataSource(formatData);
            }
        });
    }
</script>

