<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Medical Access Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @*@Styles.Render("~/Content/ej/web/flat-saffron/ej.web.all.min.css")*@
    @Styles.Render("~/Content/ej/web/bootstrap-theme/ej.web.all.min.css")
    @Scripts.Render("~/Scripts/jquery-3.1.1.min.js")
    @Scripts.Render("~/Scripts/jquery.validate.min.js")
    @Scripts.Render("~/Scripts/jsrender.min.js")
    @Scripts.Render("~/Scripts/ej/ej.web.all.min.js")
    @Scripts.Render("~/Scripts/ej/ej.unobtrusive.min.js")
    @Scripts.Render("~/Scripts/jquery.validate.unobtrusive.min.js")

    <style>
        #Wrapper {
            /*background-color: #3F51B5;*/
            z-index: 1;
            height: 500px;
            width: 250px;
            position: fixed;
        }

        #page-content-wrapper {
            margin-left: 260px;
            margin-right: 7px;
        }
    </style>

</head>
<body>

    <div class="navbar navbar-fixed-top">

        @Html.Partial("_LoginPartial")

        @Html.EJ().Menu("mainMenu").Items(items =>
   {
       items.Add().Url(Url.Action("IndexHSIP", "Home")).Text("Dashboard");
       items.Add().Url("#").Text("HSIP Orders").Children(child =>
       {

           child.Add().Url("").Text("Custom HSIP Orders").Children(child1 =>
           {
               child1.Add().Id("CustomLMIS_ART").Url(Url.Action("HSIPCustomOrders", "LMIS_ART")).Text("ART");
               child1.Add().Id("CustomLMIS_HIVTestKits").Url(Url.Action("HSIPCustomOrders", "LMIS_HIVTestKits")).Text("HIV Test Kits");
               child1.Add().Id("CustomLMIS_Laboratory").Url(Url.Action("HSIPCustomOrders", "LMIS_Laboratory")).Text("Laboratory");
               child1.Add().Id("CustomLMIS_SMC").Url(Url.Action("HSIPCustomOrders", "LMIS_SMC")).Text("SMC");
               child1.Add().Id("CustomLMIS_OISTI").Url(Url.Action("HSIPCustomOrders", "LMIS_OISTI")).Text("OIs");
               child1.Add().Id("CustomLMIS_ViralLoad").Url(Url.Action("HSIPCustomOrders", "LMIS_ViralLoad")).Text("Viral Load");
           });

           child.Add().Url("").Text("Routine HSIP Orders").Children(child1 =>
           {
               child1.Add().Id("RoutineLMIS_ART").Url(Url.Action("HSIPRoutineOrders", "LMIS_ART")).Text("ART");
               child1.Add().Id("RoutineLMIS_HIVTestKits").Url(Url.Action("HSIPRoutineOrders", "LMIS_HIVTestKits")).Text("HIV Test Kits");
               child1.Add().Id("RoutineLMIS_Laboratory").Url(Url.Action("HSIPRoutineOrders", "LMIS_Laboratory")).Text("Laboratory");
               child1.Add().Id("RoutineLMIS_SMC").Url(Url.Action("HSIPRoutineOrders", "LMIS_SMC")).Text("SMC");
               child1.Add().Id("RoutineLMIS_OISTI").Url(Url.Action("HSIPRoutineOrders", "LMIS_OISTI")).Text("OIs");
               child1.Add().Id("RoutineLMIS_ViralLoad").Url(Url.Action("HSIPRoutineOrders", "LMIS_ViralLoad")).Text("Viral Load");
           });
       });


       items.Add().Url("#").Text("HSIP Order Allocations").Children(child =>
       {
           child.Add().Url("").Text("Custom HSIP Orders Allocations").Children(child1 =>
           {
               child1.Add().Id("AllocationsLMIS_ART").Url(Url.Action("AllocationHSIPIndexCustom", "LMIS_ART")).Text("ART");
               child1.Add().Id("AllocationsLMIS_HIVTestKits").Url(Url.Action("AllocationHSIPIndexCustom", "LMIS_HIVTestKits")).Text("HIV Test Kits");
               child1.Add().Id("AllocationsLMIS_Laboratory").Url(Url.Action("AllocationHSIPIndexCustom", "LMIS_Laboratory")).Text("Laboratory");
               child1.Add().Id("AllocationsLMIS_SMC").Url(Url.Action("AllocationHSIPIndexCustom", "LMIS_SMC")).Text("SMC");
               child1.Add().Id("AllocationsLMIS_OISTI").Url(Url.Action("AllocationHSIPIndexCustom", "LMIS_OISTI")).Text("OIs");//Referral
               child1.Add().Id("AllocationsLMIS_ViralLoad").Url(Url.Action("AllocationHSIPIndexCustom", "LMIS_ViralLoad")).Text("Viral Load");//

           });

           child.Add().Url("").Text("Routine HSIP Orders Allocations").Children(child1 =>
           {
               child1.Add().Id("AllocationsLMIS_ART").Url(Url.Action("AllocationHSIPIndex", "LMIS_ART")).Text("ART");
               child1.Add().Id("AllocationsLMIS_HIVTestKits").Url(Url.Action("AllocationHSIPIndex", "LMIS_HIVTestKits")).Text("HIV Test Kits");
               child1.Add().Id("AllocationsLMIS_Laboratory").Url(Url.Action("AllocationHSIPIndex", "LMIS_Laboratory")).Text("Laboratory");
               child1.Add().Id("AllocationsLMIS_SMC").Url(Url.Action("AllocationHSIPIndex", "LMIS_SMC")).Text("SMC");
               child1.Add().Id("AllocationsLMIS_OISTI").Url(Url.Action("AllocationHSIPIndex", "LMIS_OISTI")).Text("OIs");//Referral
               child1.Add().Id("AllocationsLMIS_ViralLoad").Url(Url.Action("AllocationHSIPIndex", "LMIS_ViralLoad")).Text("Viral Load");//

           });

       });
       items.Add().Url("#").Text("HSIP SAP Export").Children(child =>
       {
           child.Add().Id("SAPART").Url(Url.Action("SAPExportHSIP", "LMIS_ART")).Text("ART");
           child.Add().Id("SAPHIV").Url(Url.Action("SAPExportHSIP", "LMIS_HIVTestKits")).Text("HIV Test Kits");
           child.Add().Id("SAPLAB").Url(Url.Action("SAPExportHSIP", "LMIS_Laboratory")).Text("Laboratory");
           child.Add().Id("SAPSMC").Url(Url.Action("SAPExportHSIP", "LMIS_SMC")).Text("SMC");
           child.Add().Id("SAPOI").Url(Url.Action("SAPExportHSIP", "LMIS_OISTI")).Text("OIs");//Referral
           child.Add().Id("SAPVL").Url(Url.Action("SAPExportHSIP", "LMIS_ViralLoad")).Text("Viral Load");//

       });

       items.Add().Url("#").Text("HSIP Queries").Children(child =>
       {
           child.Add().Id("QueriesART").Url(Url.Action("QueryHSIP", "LMIS_ART")).Text("ART");
           child.Add().Id("QueriesHIV").Url(Url.Action("QueryHSIP", "LMIS_HIVTestKits")).Text("HIV Test Kits");
           child.Add().Id("QueriesLAB").Url(Url.Action("QueryHSIP", "LMIS_Laboratory")).Text("Laboratory");
           child.Add().Id("QueriesSMC").Url(Url.Action("QueryHSIP", "LMIS_SMC")).Text("SMC");
           child.Add().Id("QueriesOI").Url(Url.Action("QueryHSIP", "LMIS_OISTI")).Text("OIs");//Referral
           child.Add().Id("QueriesVL").Url(Url.Action("QueryHSIP", "LMIS_ViralLoad")).Text("Viral Load");//

       });

       items.Add().Url("#").Text("Customer Care Unit").Children(child =>
       {
           child.Add().Id("ComplaintsCCU").Url(Url.Action("Complaints", "fo_complaint")).Text("Customer Care Unit (CCU)");
       });

       items.Add().Url("#").Text("CHSS").Children(child =>
       {
           child.Add().Id("CHSS_Dashboard").Url(Url.Action("Index", "CHSS")).Text("Dashboard");
           child.Add().Id("CHSS_PatientCareAndManagement").Url(Url.Action("PatientCareAndManagement", "CHSS")).Text("ART Patient care and management");
           child.Add().Id("CHSS_StockManagement").Url(Url.Action("StockManagement", "CHSS")).Text("Stock management");
           child.Add().Id("CHSS_Traceability").Url(Url.Action("Traceability", "CHSS")).Text("Traceability of commodities");
           child.Add().Id("CHSS_OrderingAndReporting").Url(Url.Action("OrderingAndReporting", "CHSS")).Text("Ordering and Reporting");
           child.Add().Id("CHSS_StoreManagement").Url(Url.Action("StoreManagement", "CHSS")).Text("Store management");
           child.Add().Id("CHSS_ExpiryTracking").Url(Url.Action("ExpiryTracking", "CHSS")).Text("Expiry tracking");
           child.Add().Id("CHSS_FeedBack").Url(Url.Action("FeedBack", "CHSS")).Text("Feedback details");
           child.Add().Id("CHSS_ScoringSummary").Url(Url.Action("ScoringSummary", "CHSS")).Text("Scoring summary");
       });
       items.Add().Url("#").Text("M&E").Children(child =>
       {
           //child.Add().Id("ComplaintsCCU").Url(Url.Action("Complaints", "fo_complaint")).Text("Customer Care Unit (CCU)");
       });

       items.Add().Id("Utilities").Url("#").Text("Utilities").Children(child =>
       {
           child.Add().Id("Register").Url(Url.Action("Register", "Account")).Text("Register Users");
           //child.Add().Id("ManageUsers").Url(Url.Action("ManageUsers", "Account")).Text("Manage Users");

       });

   }).EnableSeparator(false).CssClass("Black-Menu")
    </div>

    <div class="container body-content">
        @RenderSection("BreadCrumb", required: false)
        @RenderBody()
    </div>
    <footer>
        <div class="container body-content">
            <hr />
            <p>&copy; @DateTime.Now.Year - Medical Access Uganda LTD</p>
            <hr />
            <p class="box-title"><b> @String.Format("{0 : dddd, MMMM d, yyyy}", DateTime.Now) </b><span class="label label-primary"></span></p>
        </div>

    </footer>

    @Html.EJ().ScriptManager()
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <style type="text/css" class="cssStyles">
        .myclass {
            float: right;
        }
    </style>
    <script>

    </script>

    <script type="text/javascript">
        jQuery(function ($) {
            var menuObj = $("#mainMenu").ejMenu({ width: "100%", enableSeparator: false });
        });

    </script>
    <style type="text/css" class="cssStyles">
        #More, #Utilities, #Register, #Help, #ManageUsers, #Sign_In {
            float: right;
        }
    </style>

    <style type="text/css" class="cssStyles">
        .Black-Menu .e-menu, .e-menu.e-horizontal .e-list > ul {
            background: #800000;
        }

            .Black-Menu .e-menu.e-horizontal .e-list > a {
                color: white;
            }
    </style>

</body>
</html>
